<div id="container" style="min-width: 310px; width: 90%; margin: 0 auto">
  {{high-charts chartOptions=chartOptions content=chartData }}
</div>

<p class="refreshRateText">Refresh rate : every {{this.refreshRate}} seconds</p>

<button class="stopRefreshBtn" {{action "stopRefreshing" }}>{{if this.refreshing "Stop Refreshing" "Refreshing Stopped"}}</button>